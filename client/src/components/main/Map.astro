---
import Section from "../shared/Section.astro";

export interface Props {
  title: string;
}

const {title} = Astro.props as Props;

---

<Section title={title}>
  <img src="/assets/img/main/map.webp" alt="" srcset="/assets/img/main/map.webp">
</Section>

<style>
  img {
    display: block;
    width: 100%;
  }
</style>